##### Quantify reads either mapped on M. belari or E. coli

awk 'BEGIN{-F" "} $0 ~ /^>/ {print substr($1,2)} END {}' 2017_09_12_contigs_name_Ecoli.txt > contigs_short_name_Ecoli.txt &&\

awk 'BEGIN{-F" "} $0 ~ /^>/ {print substr($1,2)} END {}' 2017_09_12_contigs_name_Mbelari.txt > contigs_short_name_Mbelari.txt &&\


