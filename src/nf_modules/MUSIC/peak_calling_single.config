profiles {
  docker {
    docker.temp = 'auto'
    docker.enabled = true
    process {
      withname: compute_mappability {
        container = "music:6613c53"
      }
      withname: music_preprocessing {
        container = "music:6613c53"
      }
      withname: music_computation {
        container = "music:6613c53"
      }
    }
  }
  psmn {
    process{
      withname: compute_mappability {
        beforeScript = "module purge; module load MUSIC/6613c53"
      }
      withname: music_preprocessing {
        beforeScript = "module purge; module load MUSIC/6613c53"
      }
      withname: music_computation {
        beforeScript = "module purge; module load MUSIC/6613c53"
      }
    }
  }
}
